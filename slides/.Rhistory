library(stargazer)
control <- c(4.17, 5.58, 5.18, 6.11, 4.50, 4.61, 5.17, 4.53, 5.33, 5.14)
treated <- c(5.81, 6.17, 5.41, 6.59, 5.87, 5.83, 6.03, 4.89, 4.32, 6.69)
group <- gl(2, 10, 20, labels = c("Control", "Treated"))
weight <- c(control, treated)
m1 <- lm(weight ~ group - 1)
m2 <- lm(weight ~ group)
stargazer(m1, m2, out="table.tex", title = "My experiment", keep.stat=c("rsq","n"), no.space=TRUE, align = TRUE)
